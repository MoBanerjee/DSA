class Solution {
    public int maxDepth(String s) {
        int op=0;
        int depth=0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)!='(' && s.charAt(i)!=')') continue;
            if(s.charAt(i)=='(')op+=1;
            else op-=1;
            depth=Math.max(depth,op);
        }
        return depth;
    }
}
https://leetcode.com/problems/maximum-nesting-depth-of-the-parentheses/description/
